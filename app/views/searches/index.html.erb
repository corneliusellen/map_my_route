<% @stations.each do |station| %>
  <%= station.name %>
  <%= station.fuel_type %>
  <%= station.hours_operation %>
  Address: <%= station.street %>, <%= station.city %>, <%= station.state %>, <%= station.zip %>
  <%= link_to "Favorite Station", {controller: "favorites", action: :create, station_id: station.id, user_id: current_user.id}, method: :post %>
<% end %>
