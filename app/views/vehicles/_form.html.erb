<%= form_for @vehicle do |f| %>
  <%= f.label :make %>
  <%= f.text_field :make %>

  <%= f.label :model %>
  <%= f.text_field :model %>

  <%= f.label :year %>
  <%= f.number_field :year %>

  <%= f.label :mpg %>
  <%= f.number_field :mpg %>

  <%= f.label :fuel %>
  <%= f.select :fuel, options_for_select(fuel_types) %>

  <%= f.submit %>
<% end %>
